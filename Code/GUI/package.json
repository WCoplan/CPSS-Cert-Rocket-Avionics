{
  "name": "cpss-telemetry",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "concurrently": "^7.0.0",
    "express": "^4.17.2",
    "nodemon": "^2.0.15",
    "serialport": "^9.2.8"
  },
  "scripts": {
    "start": "node backend/index.js",
    "backend": "nodemon backend",
    "frontend": "npm start --prefix frontend",
    "dev": "concurrently -k \"npm run backend\" \"npm run frontend\"",
    "build": "cd frontend && npm install && npm run build"
  },
  "engines": {
    "node": "16.3.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
